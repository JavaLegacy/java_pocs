package org.casejpa.main;

import javax.persistence.Entity;

@Entity
public class Person {

	private Long id;
	private String name;
	private String age;

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getAge() {
		return age;
	}

	public void setAge(String age) {
		this.age = age;
	}

}
